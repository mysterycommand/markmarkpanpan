{
  "name": "markmarkpanpan",
  "version": "1.0.0",
  "description": "A new repo to present a minimal subset of files and stuff, maybe plus a few other niceties, to get his latest Panara Imports working.",
  "scripts": {
    "clean": "rm -r gh-pages/* && rm gh-pages/.*",
    "scripts": "NODE_ENV=production webpack --config=./config/webpack/build.js",
    "copy-html": "cp ./source/*.html ./gh-pages/",
    "build": "npm run clean && npm run scripts && npm run copy-html",
    "start": "webpack-dev-server --config=./config/webpack/devel.js --port=3000 --inline --hot",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mysterycommand/markmarkpanpan.git"
  },
  "author": {
    "name": "Matt Hayes",
    "email": "matt@mysterycommand.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mysterycommand/markmarkpanpan/issues"
  },
  "homepage": "https://mysterycommand.github.io/markmarkpanpan/",
  "devDependencies": {
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0"
  }
}
